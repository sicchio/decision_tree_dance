function ask() {
  "canItouchYourArm"
}





if ("yes") {
  TouchArm = true;
}

function TouchArm() {
  "TouchOtherPersonsArm"
}

if ("no") {
  TouchArm = false;
} else if ("maybe") {
  TouchArm = false;
} else {
  TouchArm = false;
}




if () {
  TouchArm = true;
} then {
  "sayHi"
}

if () {
  TouchArm = false;
} then {
  "sitDown"
}


